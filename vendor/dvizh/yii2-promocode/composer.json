{
    "name": "dvizh/yii2-promocode",
    "description": "CRUD, widget and services to give discount to clients",
    "keywords": ["yii2", "yii2", "application", "coupons", "yii2-promocode", "promocodes", "dvizh"],
    "homepage": "https://github.com/dvizh/yii2-promocode",
    "type": "yii2-extension",
    "license": "Apache",
    "support": {
        "source": "https://github.com/dvizh/yii2-promocode"
    },
    "authors": [
        {
            "name": "Ilya Pistol",
            "email": "admin@nethammer.ru"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "kartik-v/yii2-widget-datepicker": "@dev"
    },
    "config": {
        "process-timeout": 1800
    },
    "autoload": {
        "psr-4": {
            "dvizh\\promocode\\": "src\\"
        }
    },
    "extra": {
        "bootstrap": "dvizh\\promocode\\Bootstrap"
    }
}
